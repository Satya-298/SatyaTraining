package com.java.facade.example2;

public class KidneyPatientFacade {
	
	private BloodTest bloodTest;
	private HeartEchoTest heartEchoTest;
	private CreatineTest creatineTest;
	
	public KidneyPatientFacade(BloodTest bloodTest, HeartEchoTest heartEchoTest, CreatineTest creatineTest) {
		this.bloodTest = bloodTest;
		this.heartEchoTest = heartEchoTest;
		this.creatineTest = creatineTest;
	}
	
	public void info() {
		bloodTest.show();
		heartEchoTest.echo\
	}

}
