package com.java.abs1;

public class MainProg {
	
	public static void main(String[] args) {
		
		String mobile = "IPhone";
		AbstractFactory obj = new MobileFactory();
		Mobile res = obj.getMobileDetails(mobile);
		System.out.println(res.getClass().getSimpleName());
		res.name();
		res.model();
		res.price();
	}

}
