package com.java.jsf.dao;

public interface DoctorDao {
	
	package com.java.jsf.dao;

	import java.util.List;
	import com.java.jsf.model.Doctor;
	import com.java.jsf.model.Specialization;

	public interface DoctorDAO {

	    String addDoctor(Doctor doctor);

	    List<Doctor> showAllDoctors();

	    List<Doctor> showDoctorsByCategory(Specialization specialization);

	    Doctor searchDoctorById(String doctorId);
	}


}
