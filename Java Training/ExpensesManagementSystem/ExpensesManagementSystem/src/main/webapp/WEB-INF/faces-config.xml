<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee /WEB-INF/schema/web-facesconfig_2_0.xsd" 
              version="2.0" metadata-complete="false">
    <!-- This file must be present with our jar, even if it is empty.  Otherwise, our Java annotations won't get scanned! -->
<managed-bean>
	<managed-bean-name>usersDao</managed-bean-name>
    <managed-bean-class>com.java.jsf.dao.UsersDaoImpl</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
	<managed-bean-name>users</managed-bean-name>
    <managed-bean-class>com.java.jsf.model.Users</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
	<managed-bean-name>ejbUsers</managed-bean-name>
    <managed-bean-class>com.java.ejb.model.Users</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
	<managed-bean-name>login</managed-bean-name>
    <managed-bean-class>com.java.jsf.model.Login</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
	<managed-bean-name>ejbUsersImpl</managed-bean-name>
    <managed-bean-class>com.java.ejb.beans.UsersEjbImpl</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
<managed-bean-name>usersController</managed-bean-name>
    <managed-bean-class>com.java.jsf.controller.UsersController</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
    <managed-property>
            <property-name>usersDao</property-name>
            <property-class>com.java.jsf.dao.UsersDaoImpl</property-class>
            <value>#{usersDao}</value>
    </managed-property>
    <managed-property>
            <property-name>users</property-name>
            <property-class>com.java.jsf.model.Users</property-class>
            <value>#{users}</value>
    </managed-property>
    <managed-property>
            <property-name>ejbUsers</property-name>
            <property-class>com.java.ejb.model.Users</property-class>
            <value>#{ejbUsers}</value>
    </managed-property>
      <managed-property>
            <property-name>ejbUsersImpl</property-name>
            <property-class>com.java.ejb.beans.UsersEjbImpl</property-class>
            <value>#{ejbUsersImpl}</value>
    </managed-property> 
</managed-bean>



