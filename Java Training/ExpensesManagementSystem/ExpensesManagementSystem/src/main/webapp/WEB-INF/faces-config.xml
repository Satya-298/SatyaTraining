<!-- JSF Model -->
<managed-bean>
    <managed-bean-name>expenses</managed-bean-name>
    <managed-bean-class>com.java.jsf.model.Expenses</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
</managed-bean>

<!-- EJB Model -->
<managed-bean>
    <managed-bean-name>ejbExpenses</managed-bean-name>
    <managed-bean-class>com.java.ejb.model.Expenses</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
</managed-bean>

<!-- DAO -->
<managed-bean>
    <managed-bean-name>expenseDao</managed-bean-name>
    <managed-bean-class>com.java.jsf.dao.ExpenseDaoImpl</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
</managed-bean>

<!-- EJB Impl -->
<managed-bean>
    <managed-bean-name>ejbExpensesImpl</managed-bean-name>
    <managed-bean-class>com.java.ejb.beans.ExpensesEjbImpl</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
</managed-bean>

<!-- Controller -->
<managed-bean>
    <managed-bean-name>expensesController</managed-bean-name>
    <managed-bean-class>com.java.jsf.controller.ExpensesController</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
    <managed-property>
        <property-name>expenseDao</property-name>
        <property-class>com.java.jsf.dao.ExpenseDaoImpl</property-class>
        <value>#{expenseDao}</value>
    </managed-property>
    <managed-property>
        <property-name>expense</property-name>
        <property-class>com.java.jsf.model.Expenses</property-class>
        <value>#{expenses}</value>
    </managed-property>
    <managed-property>
        <property-name>ejbExpense</property-name>
        <property-class>com.java.ejb.model.Expenses</property-class>
        <value>#{ejbExpenses}</value>
    </managed-property>
    <managed-property>
        <property-name>ejbExpensesImpl</property-name>
        <property-class>com.java.ejb.beans.ExpensesEjbImpl</property-class>
        <value>#{ejbExpensesImpl}</value>
    </managed-property>
</managed-bean>

</faces-config>
